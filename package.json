{
  "name": "humanloop-docs",
  "version": "1.0.0",
  "description": "Humanloop Docs powered by Fern",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "fern docs dev || npm run dev",
    "check": "fern check",
    "watch-template": "chokidar 'fern/apis/v5/openapi/**/*' --ignore 'fern/apis/v5/openapi/openapi*.yml' -c 'python ./scripts/populate_template.py && npm run check'",
    "sync": "node ./scripts/sync_openapi.js || true && concurrently \"npm run dev\" \"npm run watch-template\""
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "fern-api": "^0.37.6",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.4",
    "prettier": "^3.3.3"
  },
  "author": "",
  "license": "ISC"
}
