{
  "name": "humanloop-docs",
  "version": "1.0.0",
  "description": "Humanloop Docs powered by Fern",
  "main": "index.js",
  "scripts": {
    "dev": "fern docs dev",
    "check": "fern check",
    "watch": "chokidar 'fern/apis/v5/openapi/**/*' --ignore 'fern/apis/v5/openapi/openapi*.yml' -c 'python ./scripts/populate_template.py && npm run check'",
    "dev-api": "./scripts/sync_openapi.sh && concurrently \"npm run dev\" \"npm run watch\""
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
