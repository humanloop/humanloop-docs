{
  "name": "humanloop-docs",
  "version": "1.0.0",
  "description": "Humanloop Docs powered by Fern",
  "main": "index.js",
  "scripts": {
    "dev": "fern docs dev",
    "check": "fern check",
    "watch-template": "chokidar 'fern/apis/v5/openapi/**/*' --ignore 'fern/apis/v5/openapi/openapi*.yml' -c 'python ./scripts/populate_template.py && npm run check'",
    "sync": "./scripts/sync_openapi.sh || true && concurrently \"npm run dev\" \"npm run watch-template\""
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0"
  },
  "author": "",
  "license": "ISC"
}
